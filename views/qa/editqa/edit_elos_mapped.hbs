
<div class="container">       
    <form class="form-horizontal" role="form"  action="/add_aun1-3?program={{program}}" method="post" id = "myForm" > 
                
          <h3>Mapped the expected learning outcomes (ELOs)</h3>
          <br>
              <div class="form-group">
                
                
               <label for="inputType" class="col-md-2 control-label">Category: </label>
                <div class="col-md-4">
                    <input type="text" class="form-control" id="inputType" placeholder="Enter categoey" name="category" required value = "{{respon.category}}">
                </div>  
              </div>
              <div class="form-group">
                <label for="inputType" class="col-md-2 control-label">Description: </label>
                <div class="col-md-4">
                    <textarea class="form-control" rows="5" id="des" name='description'>{{respon.description}}</textarea>
                </div>
                <label for="inputType" class="col-md-2 control-label">ELOs: </label>
                
                
                <div class="col-md-4">

                {{#each elo}}
                  <input type="checkbox" name="elo_tqf2" value="{{_id}}"> {{number}}<br>
                {{/each}}

                </div>
                  

                  
               

                
              </div>

              <div class="form-group">
              <div class="pull-right">
                      <p align = "right"><button type="submit" class="btn btn-primary">Add</button></p>
             </div>
             </div>
              


     </form>
</div>