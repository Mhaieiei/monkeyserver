



    
    <form class="form-horizontal" role="form"  action="/aun/aun1-3/add_aun1-3?&program={{getprogram}}&year={{getyear}}&acid={{getacid}}" method="post" id = "myForm" > 
         <div class="container-fluid">
            <div class="row-fluid">
            <div class="span15">
            <div class="col-md-15"> 
                
          <h3>Mapped The expected learnig outcomes (ELOs)</h3>
          <br>
              <div class="form-group">
                
                
               <label for="inputType" class="col-md-1 control-label">Category: </label>
                <div class="col-md-4">
                    <input type="text" class="form-control" id="inputType" placeholder="Enter categoey" name="category" required>
                </div>  
              </div>
              <div class="form-group">
                <label for="inputType" class="col-md-1 control-label">Description: </label>
                <div class="col-md-4">
                    <textarea class="form-control" rows="5" id="des" name='description'></textarea>
                </div>
                <label for="inputType" class="col-md-2 control-label">ELOs: </label>
                
                
                <div class="col-md-4">

                {{#each elo}}
                  <input type="checkbox" name="elo_tqf2" value="{{_id}}"> {{number}}<br>
                {{/each}}

                </div>                 
              </div>

              <div class="col-md-11">
                <div class="pull-right">
             
                 <button type="submit" class="btn btn-primary">Add</button>
                 </div>
             </div>
            </div>
            </div>
            </div>
              


     </form>
</div>