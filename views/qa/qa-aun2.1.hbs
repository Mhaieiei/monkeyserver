

<body>

    
        <td>
            <div class="container-fluid">
            <div class="row-fluid">
            <div class="span15">
            <div class="col-md-15"> 
           
            <!--<form class="form-horizontal" role="form">-->
                
                <h3>AUN 2.1 The university uses program specification</h3>
               
                <br>
				<h5>Table : Referenved curriculums from the world recognized universities</h5>
                <div id="tab_ref">
                  <table class="tablesorter">
                    <thead align="center">
                        <tr>
                            <th>No.</a></th>
                            <th>Program Name</a></th>
                            <th>Degree</a></th>
                            <th>University</a></th>
                            <th>Country</a></th>
                            <th>Websites</a></th>

                        </tr>
      
                    </thead>
					 {{#each docs}}
                    <tbody>
					{{#each referenceCurriculum}}
					{{#each detail}}
                        <tr>
                        <td>{{getindex}} </td>
                        <td> {{refProgramName}}</td>
                        <td>{{degree}} </td>
                        <td> {{university}}</td>
                        <td> {{country}}</td>
                        <td>{{website}} </td>
                        
                        </tr>
                       {{/each}}
                        {{/each}}
                    </tbody>
					{{/each}} 
                  </table>
                </div>


				{{#each docs}}
				{{#each structureOfCurriculum}}
                <h5>Table : Structure of the curriculum (Plan {{plan}})</h5>
                <div id="tab_structure">
                  <table class="tablesorter">
                    <thead align="center">
                        <tr>
                            <th>Knowledge Block</a></th>
                            <td colspan=2 >{{plan}}</a></td>
                        </tr>
                        <tr>
                            <td colspan=1></td>
                            <td colspan=1></td>
                            <th>Credit Required</th>
                            <th>Percentage</th>
                           
                        </tr>
      
                    </thead>
					{{#each knowledgeBlock}}
                    <tbody>
                        <tr>
                        <td> {{subjectType}}</td>
                        <td> </td>
                        <td>{{creditRequired}} </td>
                        <td> </td>
                        
                        </tr>
                       
                        
                    </tbody>
					{{/each}}
                  </table>
                </div>
				{{/each}}
                {{/each}}

                <h5>Table : Distribution of the curriculum</h5>
                <div id="tab_dis">
                  <table class="tablesorter">
                    <thead align="center">
					{{#each subs}}
                        <tr>
                            <td colspan=3 >Semester {{semester}}</a></td>
                        </tr>
                        <tr>
                            <td>Code</td>
                            <td>Subject</td>
                            <td>Credit</td>
                        </tr>
                    <!-- ใช้ th colspan กับ Plan ใช้ td กับข้อมูลปกติ -->
                    </thead>
                    <tbody>
                        <tr>
						<td colspan=3>{{plan.plan}} : Year {{Year}}</td>
						</tr>
						{{#each subject}}
						{{#each subcode}}
                        <tr>
						

						
                        <td>{{sub_code}}</td>
                        <td>{{sub_name}}</td>
                        <td>{{sub_credit}}</td>
						
                        </tr>    
                       {{/each}}
					   {{/each}} 
                    </tbody>
					{{/each}}
                  </table>
                </div>
          
            </div>
            </div>
            </div>
            </div>
            

        </td>



         
</body>

