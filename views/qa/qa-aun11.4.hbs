
<body>

    
        <td>
            <div class="container-fluid">
            <div class="row-fluid">
            <div class="span15">
            <div class="col-md-15"> 
           
            <!--<form class="form-horizontal" role="form">-->
                
                <h3>AUN 11.4: The curriculum is regurly evaluated at reasonable time periods</h3>
       
                <h5>Table : Program evaluation methods and frequencies</h5>
                <div id="tab_aun11.4">
                  <table class="tablesorter">
                    <thead align="center">
                        <tr>
                            
                            <th>No.</a></th>
                            <th>Stakeholders</a></th>
                            <th>Methods/Tools</a></th>
                            <th>Frequency</a></th>
                        </tr>
                             

                    <!-- ใช้ td colspan กับ Plan / topic ใช้ td กับข้อมูลปกติ -->
                    </thead>
					{{#each docs}}
					{{#each evaluation}}
                    <tbody>
                        <tr>
                        <td>{{getindex}}</td>

                        <td>{{stakeholder.title}}</td>
						{{#each EvaluationMethod}}
                        

						{{#if @first}} 
                <td>{{methodName}}</td>
                <td>{{frequency}}</td>
                </tr>
			 	
				{{else}}
                <tr>
                <th></th>
				<th></th>
                <td>{{methodName}}</td>
                <td>{{frequency}}</td>
				{{/if}}
                        {{/each}}
                        </tr>  
                        
                    </tbody>
					{{/each}}
					{{/each}}   
                  </table>

                </div>
          
            </div>
            </div>
            </div>
            </div>
            

        </td>



         
</body>
