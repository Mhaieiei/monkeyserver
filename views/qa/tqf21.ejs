<%- include ../headerqa.ejs %>


        
   <h3>Program faculty members</h3>
      <h5>Table : List of faculty member in <%=programname%> program</h5> 
	     <table width="100%" border="0">
                <div id="tab_tqf1"><table class="tablesorter">
                <thead align="center">
                    <tr>
                        <th width="40"><a href="javascript:table_sort('tab_2',0)">No.</a></th>
                        <th><a href="javascript:table_sort('tab_tqf1',1)">Title</a></th>
                        <th><a href="javascript:table_sort('tab_tqf1',2)">Name-Surname</a></th>
                        <th colspan=4><a href="javascript:table_sort('tab_tqf1',3)">Education</a></th>
                    </tr>
		
                    <tr>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th>Level</th>
                      <th>Degree</th>
                      <th>University</th>
                      <th>Year</th>
                    </tr>


	
                </thead>
                <tbody>
              <% var i,j;
            		for(i=0;i< clients.length;i++){
                  if(clients[i].education != null){
            			for(j=0;j< clients[i].education.length;j++){%>
            			<tr>
            			   
            			   <% if(j==0) {%>
            			   <td><%= i+1%></td>
            			   <td><%= clients[i].local.title %></td>
            			   <td><%= clients[i].local.name %></td>
            			   <%} else {%>
            			   <td></td>
            			   <td></td>
            			   <td></td>
            			   <%}%>
            			   <td><%= clients[i].education[j].level %></td>
            			   <td><%= clients[i].education[j].degree %></td>
            			   <td><%= clients[i].education[j].university %></td>
            			   <td><%= clients[i].education[j].year %></td>
            			</tr>
            		<%  }
                  }
            		}%>
                  
                    
                </tbody>
                </div>
      </table>
                         
   
	
<%- include ../footer.ejs %>