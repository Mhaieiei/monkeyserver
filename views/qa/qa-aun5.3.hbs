<div class="container">
  <form class="form-horizontal" role="form"  action="//aun5-3" method="post" id = "myForm" >  

    
    <h3>AUN 5.3: Student assessment uses a variety of methods</h3>
    <h5>Table : Assesment tools</h5>
    <table id="rowspan" cellspacing="0" class="tablesorter">
        <thead>
            <tr>
                
                <tr>
                        <td>Edit</td>
                        <td>Delete</td>
                        <th>No.</a></th>
                        <th>Assessment Tools</a></th>
                        <td>Courses</a></td>
                        <td>Levels</a></td>
                      
                </tr>
                
            
            </tr>
            
        </thead>
        <tbody>     

    	{{#each docs}}
    	<tr>
    	<td colspan=4>{{_id}}</td>
    	</tr>
                    <tr>
            			{{#each assementTool}}

                        <td><a href = "/aun/aun5-3/edit_aun5-3?id={{_id}}&program={{getprogram}}&year={{getyear}}&acid={{getacid}}"><button id="tab_2_submit" class="btn btn-mini" type="button">Edit</button></td>

                        <td><a href = "/aun/aun5-3/del_aun5-3?id={{_id}}&program={{getprogram}}&year={{getyear}}&acid={{getacid}}"><button id="tab_2_submit" class="btn btn-mini" type="button">Delete</button></td>
                          
                        <th>{{inc @index}}</th>

        				

                        <th>{{assesmentTool}}</th>
            				
            			{{#each subject}} 
            			{{#if @first}} 
                        
                            <td>{{subjectType}}</td>
                            <td>{{followingReq}}</td>
                            
                    </tr>
    			 	
    				{{else}}
                    <tr>
                    <th></th>
                    <th></th>
    				<th></th>
                    <th></th>
                    <td>{{subjectType}}</td>
                    <td>{{followingReq}}</td>

    				{{/if}}
    				{{/each}}
    				
    				</tr>
    				{{/each}}
         {{/each}}   

                    

                    
                    
                    
         </tbody>
         <a href = "/aun/aun5-3/add_aun5-3?program={{programname}}&year={{year}}&acid={{acid}}"><button id="tab_2_submit" class="btn btn-mini" type="button">Add</button>

     </table>


    </form>
</div>

