
<body>

    
        <td>
            <div class="container-fluid">
            <div class="row-fluid">
            <div class="span15">
            <div class="col-md-15"> 
           
            <!--<form class="form-horizontal" role="form">-->
                
                <h2>AUN 7: Support Staff Quality</h2>
       
                <h5>Table : List of support staffs of International College</h5>
                <div id="tab_aun7">
                  <table class="tablesorter">
                    <thead align="center">
                        <tr>
                            
                            <th>No.</a></th>
                            <th>Title</a></th>
                            <th>Name-Surname</a></th>
                            
                            <th>Education</a></th>
                            <th>Position</a></th>
                            <th>Job Description</a></th>
                            
                            

                        </tr>
                       

                    <!-- ใช้ th colspan กับ Plan / topic ใช้ td กับข้อมูลปกติ -->
                    </thead>
					{{#each docs}}
                    <tbody>
					{{#each user}}
                        <tr>
                        <td>{{getindex}}</td>
                        <td>{{local.title}}</td>
                        <td>{{local.name}} {{local.surname}}</td>
                        
                        {{#each education}}
                        <td>{{level}}</td>
                        {{/each}}
                        {{/each}}
                        <td>{{position}}</td>
                        <td>{{jobDescription}}</td>
                        </tr>  
                        
                       
                    </tbody>
					{{/each}}
                  </table>

                </div>

                <h5>Table : Termination and retirement information of support staffs</h5>
                <div id="tab_aun6.9">
                  <table class="tablesorter">
                    <thead align="center">
                        <tr>
                            
                            <th>No.</a></th>
                            <th>Title</a></th>
                            <th>Name-Surname</a></th>
                            <th>Age</a></th>
                            <td colspan=2>Termination</a></td>
                            <td colspan=2>Retirement</a></td>
                        </tr>
                        <tr>
                            <td colspan=1></td>
                            <td colspan=1></td>
                            <td colspan=1></td>
                            <td colspan=1></td>
                            <th> Remaining Times (years)</th>
                            <th> Academic Years</th>
                            <th> Remaining Times (years)</th>
                            <th> Academic Years</th>

                        </tr>
                       

                    <!-- ใช้ td colspan กับ Plan / topic ใช้ td กับข้อมูลปกติ -->
                    </thead>
					
                    <tbody>
					
                        <tr>
                        <td colspan=8></td>
                        </tr>
                      {{#each docs}}
                       {{#each user}}
                        <tr>
                        <td>{{getcount}}</td>
                        <td>{{local.title}}</td>
                        <td>{{local.name}} {{local.surname}}</td>
                        <td>{{local.age}}</td>
                        <td>{{getTerminate local.terminationYear}}</td>
                        <td>{{local.terminationYear}}</td>
                        <td>{{getRetired local.age}}</td>
                        <td>{{getAcYearOfRetired local.age}}</td>
                        </tr>  
                        {{/each}}
                        {{/each}}
                    </tbody>
					
                  </table>
				  
                </div>
          
            </div>
            </div>
            </div>
            </div>
            

        </td>



         
</body>

