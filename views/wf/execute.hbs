<br><br><br>
<center> <h1><strong>Template Workflow List</strong></h1></center>

<br><br><br>

<table width ="900" border = "0" align = "center">
	<tr>
		<td>
			
			<table  class="table table-bordered" >
				<tr>
					<th> Name </th>
					<th> Description </th>
					<th> Executable</th>
				</tr>
				{{#each workflows}}
				<tr>
					<td width = "400"> 
						<div style ="font-size: 19px">{{this.name}}</div>
					</td>
					<td width = "400">
						<div>{{this.description}}</div>
					</td>
					<td width = "200">
						<div>
							<a href="/workflow/{{this._id}}/profile" class="btn-sm btn-primary">Execute</a>
							<a href="/workflow/{{this._id}}/edit" class="btn-sm btn-primary">Edit</a>
							<a href="/workflow/{{this._id}}/delete" class="btn-sm btn-primary">Delete</a>
						</div>
					</td>
	
				</tr>
				{{/each}}
			  
			</table>
		
		</td>
	</tr>
</table>
