



<form action="/addmeeting" method="post" id = "myForm" >	

	<div class="row content"> 
                <form class="form-horizontal" role="form">
                    </div>

                     <div class="form-group">
                      <label for="inputType" class="col-md-2 control-label">Meeting Date :</label>
                      <div class="col-md-4">
                          <input type="text" class="form-control" id="inputType" placeholder="Enter Meeting Date" name="meetingday" required>
                      </div>
                    </div>

                    <div class="form-group">
                      <label for="inputType" class="col-md-2 control-label">Acedemic Year :</label>
                      <div class="col-md-4">
                          <input type="text" class="form-control" id="inputType" placeholder="Enter academic year" name="acyear" required>
                      </div>
                    </div>

                     <div class="form-group">
                      <label for="inputType" class="col-md-2 control-label">No. of Participation :</label>
                      <div class="col-md-4">
                          <input type="text" class="form-control" id="inputType" placeholder="Enter number of participation" name="participation" required>
                      </div>
                    </div>

                    <div class="form-group">
                      <label for="inputType" class="col-md-2 control-label">Percentage. of Participation :</label>
                      <div class="col-md-4">
                          <input type="text" class="form-control" id="inputType" placeholder="Enter number percentage of participation" name="percentpart" required>
                      </div>
                    </div>

                    

                    <p hidden>		
					  <input type="text" tabindex="1"  name="arrlen" id="arrlen" value='1' readonly=""><br>
					  <input type="text" tabindex="1" name="program" id="program" value="{{program}}"><br>
					</p>
			                   
    				
                    
                    </div>
                     <div class="btn-group">
                            <button id="tab_2_submit" class="btn btn-mini" type="submit">Submit</button>
                     </div>



	  
</form>