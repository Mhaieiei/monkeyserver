
 <div class="container">
  <form class="form-horizontal" role="form"  action="/addmeeting" method="post" id = "myForm" >	
    <br><br>
               
           <div class="form-group">
              <label for="fromDate" class="col-md-2 control-label">Meeting Date :</label>
              <div class="col-md-4">
                   <input type="date" class="form-control" id="fromDate" name="fromDate" value="">
              </div>

              <label for="inputType" class="col-md-2 control-label">Acedemic Year :</label>
              <div class="col-md-4">
                  <input type="text" class="form-control" id="inputType" placeholder="Enter academic year" name="acyear" required>
              </div>
          </div>
           
          <div class="form-group">
              <label for="inputType" class="col-md-2 control-label">No. of Participation :</label>
              <div class="col-md-4">
                  <input type="number" class="form-control" id="inputType" placeholder="Enter number of participation" name="participation" required>
              </div>
              
              <label for="inputType" class="col-md-2 control-label">Percentage. of Participation :</label>
              <div class="col-md-4">
                  <input type="number" min="0" max="100" step="10" class="form-control" id="inputType" placeholder="Enter number percentage of participation" name="percentpart" required>
              </div>
          </div>

                      

          <p hidden>		
  					  <input type="text" tabindex="1"  name="arrlen" id="arrlen" value='1' readonly=""><br>
  					  <input type="text" tabindex="1" name="program" id="program" value="{{program}}"><br>
  				</p>
  			                   
      				
                      
          <div class="pull-right">
               <div class="btn-group">
                      <button id="tab_2_submit" class="btn btn-mini" type="submit">Submit</button>
               </div>
          </div>



  	  
  </form>
</div>