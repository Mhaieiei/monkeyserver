
 <div class="container">
  <form class="form-horizontal" role="form"  action="/admin/editmeeting" method="post" id = "myForm" >	
    <br><br>
          
           <div class="form-group">
              <label for="fromDate" class="col-md-2 control-label">Meeting Date :</label>
              <div class="col-md-4">
                   <input type="date" class="form-control" id="fromDate" name="fromDate" value="{{meeting.meetingDate}}">
              </div>

              
          </div>
           
          <div class="form-group">
              <label for="inputType" class="col-md-2 control-label">No. of Participation :</label>
              <div class="col-md-4">
                  <input type="text" class="form-control" id="inputType" name="participation" value="{{meeting.noOfParticipation}}" required>
              </div>
              
              <label for="inputType" class="col-md-2 control-label">Percentage. of Participation :</label>
              <div class="col-md-4">
                  <input type="text" class="form-control" id="inputType"  name="percentpart" value="{{meeting.percentageOfParticipation}}"required>
              </div>
          </div>

                      

        <p hidden>		
			   <input type="text" tabindex="1" name="acyear" id="acyear" value="{{acid}}"><br>
         <input type="text" tabindex="1" name="workid" id="workid" value="{{meeting._id}}"><br>
  			</p>
  			                   
      				
                      
          <div class="pull-right">
               <div class="btn-group">
                      <button id="tab_2_submit" class="btn btn-mini" type="submit">Submit</button>
               </div>
          </div>



  	  
  </form>
</div>