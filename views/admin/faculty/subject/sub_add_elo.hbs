<div class="container">
           
    <form class="form-horizontal" role="form"  action="/admin/sub_elo?acid={{acid}}&sub_id={{sub_id}}" method="post" id = "myForm" > 
            <!--<form class="form-horizontal" role="form">-->
                
                <h3>Add ELO to this subject</h3>
                
            

                    
                    <h4>Code : {{sub_code}}</h4>
                    <h4>Subject name : {{sub_name}}</h4>
                    <h4>Credit : {{sub_credit}}</h4>
                    <div class="form-group">

                        <label for="inputType" class="col-md-2 control-label">ELOs and Support levels : </label>

                        <p hidden>    
                        <input type="text" tabindex="1"  name="arrlen" id="arrlen" value='{{len}}' readonly=""><br>
                       
                      </p>
                          <div class="col-md-4">
                            <table class="table muffin table-striped table-hover margin-bottom0" id="elosTable">
                              <thead>
                                  <tr>
                                  <th ><a >ELOs</a></th>
                                  <th ><a >Support levels</a></th>
                                  
                                  
                                  </tr>
                              </thead>


                              <tbody id="elosbody">


                              {{#each elo}}
                              <p hidden>    
                                <input  type="text" name="elo" id="{{number}}" value="{{_id}}" /></td>
                                 </p>
                                <tr>
                                <td><label for="{{number}}" class="col-md-2 control-label">{{number}} </label></td>

                                <td>
                                  <select style="width:200px" id="supportlevels" name="supportlevels">
                                      <option value="Highly Supportive">Highly Supportive</option>
                                      <option value="Supportive">Supportive</option>
                                      <option value="None">None</option>
                                </td>
                                      
                                </tr> 
                                {{/each}}
                              </tbody>
                            </table>
                          </div>
                   </div>
  
                    <div class="form-group">
                    <button type="submit" class="btn btn-primary">Add</button>
                  </div>
    </form>
<div>